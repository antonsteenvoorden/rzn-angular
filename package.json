{
  "name": "rzn-angular",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test --code-coverage & npm run remap-istanbul",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "ng build -prod",
    "remap-istanbul": "remap-istanbul -i ./coverage/coverage-final.json -o ./report/coverage/html-report -t html",
    "coverage-report": "http-server -c-1 -o -p 9875 ./report/coverage/html-report",
    "unittest-report": "http-server -c-1 -o -p 9876 ./report/unittest/html-report",
    "e2e-report": "http-server -c-1 -o -p 9876 ./report/e2e/html-report"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~2.1.0",
    "@angular/compiler": "~2.1.0",
    "@angular/core": "~2.1.0",
    "@angular/forms": "~2.1.0",
    "@angular/http": "~2.1.0",
    "@angular/material": "^2.0.0-alpha.10",
    "@angular/platform-browser": "~2.1.0",
    "@angular/platform-browser-dynamic": "~2.1.0",
    "@angular/router": "~3.1.0",
    "angular2-google-maps": "^0.16.0",
    "angular2-material-datepicker": "https://github.com/zaijacobs/Angular-2-Datepicker/tarball/master",
    "core-js": "^2.4.1",
    "hammerjs": "^2.0.8",
    "http-server": "^0.9.0",
    "ng2-datepicker": "https://github.com/zaijacobs/ng2-datepicker/tarball/master",
    "ng2-pagination": "^1.0.1",
    "ng2-validation": "^3.0.1",
